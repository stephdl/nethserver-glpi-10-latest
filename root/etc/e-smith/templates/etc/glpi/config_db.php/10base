{
use strict;
use NethServer::Password;
my $db_pass = NethServer::Password::store('glpi');

$OUT .= qq(
 class DB extends DBmysql {
                
 var \$dbhost = '127.0.0.1:8105';
                
 var \$dbuser 	= 'glpi';
                
 var \$dbpassword= '$db_pass';
                
 var \$dbdefault = 'glpi';
                
 } 
?>);
}
